<ul>
	{{# this}}
	    <li class="isOver">
	      <a external _href="/activity/activityDetail.html?id={{id}}" class="item-link external item-content">
	        <div class="item-media">
	        	{{#each imgList}}
	          		<img src="{{imageUrl}}" style='width: 4.5rem;'>
	        	{{/each}}
	        </div>
	        <div class="item-inner">
	          <div class="item-title-row">
	            <div class="item-title">{{title}}</div>
	            <div class="item-after">{{address}}</div>
	          </div>
	          <div class="item-subtitle">时间:{{startTimeStr}}</div>
	          <div class="item-text">{{description}}</div>
	        </div>
	      </a>
	    </li>
	{{/this}}
</ul>